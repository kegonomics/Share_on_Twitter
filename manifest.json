{
   "background": {
      "persistent": false,
      "scripts": [ "tweetright.js" ]
   },
   "browser_action": {
      "default_icon": "32.png",
      "default_title": "Share on Twitter"
   },
   "description": "Share web pages, selected text, links and images on Twitter easily from the context menu and the toolbar button.",
   "icons": {
      "128": "128.png",
      "16": "16.png",
      "32": "32.png",
      "48": "48.png"
   },
   "manifest_version": 2,
   "name": "Share on Twitter",
   "permissions": [ "contextMenus", "activeTab" ],
   "homepage_url": "http://browsernative.com/extensions/",
   "version": "0.99"
}